<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <path
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Logo URL"
                        name="./imagePath"
                        xtype="pathfield"/>
                    <textoflogo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Enter Dashboard name"
                        fieldLabel="Text Of Logo"
                        name="./imageName"/>
                    <definitions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Url"
                        name="./definitions"
                        orderable="{Boolean}true"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <term
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Label"
                                    key="term"
                                    width="250"
                                    xtype="pathfield"/>
                                <link
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Link"
                                    key="definition"
                                    width="250"
                                    xtype="pathfield"/>
                            </items>
                        </fieldConfig>
                    </definitions>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Tab 2">
                <items jcr:primaryType="cq:WidgetCollection">
                    <definitions
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Url"
                        name="./url"
                        orderable="{Boolean}true"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="cq:Widget"
                            xtype="multifieldpanel">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <term
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Label"
                                    key="term"
                                    width="250"
                                    xtype="pathfield"/>
                                <link
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Link"
                                    key="definition"
                                    width="250"
                                    xtype="pathfield"/>
                            </items>
                        </fieldConfig>
                    </definitions>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
